<?xml version="1.0" encoding="UTF-8"?>
<testlist>
    <test name="Invoke service by identifier">
        <request>
            <identifier>res:/songlift/api/World</identifier>
        </request>
        <assert>
            <stringEquals>Hello World</stringEquals>
        </assert>
    </test>
    
    <test name="Invoke service by endpoint">
        <request>
            <identifier>meta:songlift:api</identifier>
            <argument name="something">World</argument>
        </request>
        <assert>
            <stringEquals>Hello World</stringEquals>
        </assert>
    </test>
</testlist>
